   ],
   "source": [
    "# --- Enhanced Ideal Line Calculation with Gap Filling ---\n",
    "# This cell defines a helper to build sector segments by filling missing Normalized_Index\n",
    "# positions from the next-best laps for that sector, and a new calculate_ideal_line_by_sectors_v2\n",
    "# that you can run instead of the original. It will try to use the existing plot_ideal_line\n",
    "# if present; otherwise it will draw a basic plot.\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "from scipy.interpolate import interp1d\n",
    "\n",
    "\n",
    "def build_sector_segment(df, sector_num, start_idx, end_idx, sector_ranked_laps, max_interp_gap=5):\n",
    "    \"\"\"Build a sector segment covering integer Normalized_Index positions from start_idx..end_idx.\n",
    "    Attempts to fill missing indices by taking points from the ranked laps for that sector, then\n",
    "    interpolates short gaps using other available points for the sector.\n",
    "    Returns a DataFrame with the same columns as df (may contain NaNs for some fields).\n",
    "    \"\"\"\n",
    "    desired = np.arange(int(start_idx), int(end_idx) + 1, dtype=int)\n",
    "\n",
    "    collected = pd.DataFrame(columns=df.columns.tolist() + ['SourceLap'])\n",
    "    collected_indices = np.array([], dtype=int)\n",
    "\n",
    "    # Iterate candidate laps (best first)\n",
    "    for lap in sector_ranked_laps:\n",
    "        lap_points = df[(df['lap'] == lap) & (df['Sector'] == sector_num)].copy()\n",
    "        if lap_points.empty:\n",
    "            continue\n",
    "        # create integer Normalized_Index to match\n",
    "        lap_points = lap_points.copy()\n",
    "        lap_points['NormIdx_int'] = lap_points['Normalized_Index'].round(0).astype(int)\n",
    "\n",
    "        missing = np.setdiff1d(desired, collected_indices, assume_unique=False)\n",
    "        if missing.size == 0:\n",
    "            break\n",
    "\n",
    "        take_mask = lap_points['NormIdx_int'].isin(missing)\n",
    "        take = lap_points[take_mask].copy()\n",
    "        if take.empty:\n",
    "            continue\n",
    "\n",
    "        take['SourceLap'] = lap\n",
    "        take['Normalized_Index'] = take['NormIdx_int'].astype(float)\n",
    "        collected = pd.concat([collected, take], ignore_index=True, sort=False)\n",
    "        collected_indices = np.unique(np.concatenate([collected_indices, take['NormIdx_int'].astype(int).unique()]))\n",
    "\n",
    "        if np.setdiff1d(desired, collected_indices).size == 0:\n",
    "            break\n",
    "\n",
    "    # Interpolate short missing gaps\n",
    "    missing_after = np.setdiff1d(desired, collected_indices)\n",
    "    if missing_after.size > 0:\n",
    "        available = df[df['Sector'] == sector_num].copy()\n",
    "        if not available.empty:\n",
    "            available = available.copy()\n",
    "            available['NormIdx_int'] = available['Normalized_Index'].round(0).astype(int)\n",
    "            avail_pts = available.drop_duplicates(subset='NormIdx_int').sort_values('NormIdx_int')\n",
    "            avail_idx = avail_pts['NormIdx_int'].values.astype(int)\n",
    "            avail_x = avail_pts['X'].values.astype(float)\n",
    "            avail_y = avail_pts['Y'].values.astype(float)\n",
    "\n",
    "            # group contiguous missing indices\n",
    "            from itertools import groupby\n",
    "            groups = []\n",
    "            for k, g in groupby(enumerate(missing_after), lambda ix: ix[0] - ix[1]):\n",
    "                grp = list(map(lambda t: t[1], g))\n",
    "                groups.append(grp)\n",
    "\n",
    "            interp_rows = []\n",
    "            if len(avail_idx) >= 2:\n",
    "                f_x = interp1d(avail_idx, avail_x, bounds_error=False, fill_value='extrapolate')\n",
    "                f_y = interp1d(avail_idx, avail_y, bounds_error=False, fill_value='extrapolate')\n",
    "\n",
    "                for grp in groups:\n",
    "                    if len(grp) <= max_interp_gap:\n",
    "                        xi = np.array(grp, dtype=int)\n",
    "                        new_x = f_x(xi)\n",
    "                        new_y = f_y(xi)\n",
    "                        for idx_i, nx, ny in zip(xi, new_x, new_y):\n",
    "                            row = {c: np.nan for c in df.columns}\n",
    "                            row.update({'lap': np.nan, 'Sector': sector_num, 'Normalized_Index': float(idx_i), 'X': float(nx), 'Y': float(ny), 'SourceLap': np.nan})\n",
    "                            interp_rows.append(row)\n",
    "\n",
    "            if interp_rows:\n",
    "                interp_df = pd.DataFrame(interp_rows)\n",
    "                collected = pd.concat([collected, interp_df], ignore_index=True, sort=False)\n",
    "                collected_indices = np.unique(np.concatenate([collected_indices, np.array([int(x) for x in interp_df['Normalized_Index']])]))\n",
    "\n",
    "    remaining_missing = np.setdiff1d(desired, collected_indices)\n",
    "    if remaining_missing.size > 0:\n",
    "        print(f\"Warning: Could not find data for Sector {sector_num} indices: {remaining_missing[:10]}... (total {remaining_missing.size})\")\n",
    "\n",
    "    if not collected.empty:\n",
    "        collected['NormIdx_int'] = collected['Normalized_Index'].round(0).astype(int)\n",
    "        collected = collected.sort_values(by='NormIdx_int').drop_duplicates(subset='NormIdx_int', keep='first').reset_index(drop=True)\n",
    "        collected['Normalized_Index'] = collected['NormIdx_int'].astype(float)\n",
    "        # keep only intersection columns\n",
    "        cols = [c for c in df.columns if c in collected.columns]\n",
    "        cols += [c for c in ['SourceLap'] if c in collected.columns]\n",
    "        return collected[cols].copy()\n",
    "    else:\n",
    "        return pd.DataFrame(columns=df.columns)\n",
    "\n",
    "\n",
    "def calculate_ideal_line_by_sectors_v2(df, max_interp_gap=5, smoothness_param=50):\n",
    "    \"\"\"Alternative implementation that fills gaps from next-best laps for each sector.\"\"\"\n",
    "    df = df.copy()\n",
    "    df = df.sort_values(by=['lap', 'Normalized_Index']).copy()\n",
    "\n",
    "    TARGET_S1_DIST_IN = 40512\n",
    "    TARGET_S2_DIST_IN = 102732\n",
    "\n",
    "    s1_boundary_points = df[(df['LapDist_in'].notna()) & (df['LapDist_in'] >= TARGET_S1_DIST_IN - 500) & (df['LapDist_in'] <= TARGET_S1_DIST_IN + 500)]\n",
    "    s2_boundary_points = df[(df['LapDist_in'].notna()) & (df['LapDist_in'] >= TARGET_S2_DIST_IN - 500) & (df['LapDist_in'] <= TARGET_S2_DIST_IN + 500)]\n",
    "\n",
    "    SECTOR_1_END_INDEX = int(round(s1_boundary_points['Normalized_Index'].median())) if not s1_boundary_points.empty else int(df['Normalized_Index'].max() / 3)\n",
    "    SECTOR_2_END_INDEX = int(round(s2_boundary_points['Normalized_Index'].median())) if not s2_boundary_points.empty else int((df['Normalized_Index'].max() / 3) * 2)\n",
    "    MAX_TRACK_INDEX = int(df['Normalized_Index'].max())\n",
    "\n",
    "    def assign_sector(normalized_index):\n",
    "        if pd.isna(normalized_index): return None\n",
    "        if normalized_index <= SECTOR_1_END_INDEX: return 1\n",
    "        elif normalized_index <= SECTOR_2_END_INDEX: return 2\n",
    "        else: return 3\n",
    "\n",
    "    df['Sector'] = df['Normalized_Index'].apply(assign_sector)\n",
    "\n",
    "    # Compute sector performance per lap\n",
    "    sector_performance = []\n",
    "    for lap_num in sorted(df['lap'].unique()):\n",
    "        lap_data = df[df['lap'] == lap_num].copy()\n",
    "        sectors_in_lap = lap_data['Sector'].nunique()\n",
    "        if sectors_in_lap < 3:\n",
    "            continue\n",
    "        s1_speed = lap_data[lap_data['Sector'] == 1]['Speed_kph'].mean()\n",
    "        s2_speed = lap_data[lap_data['Sector'] == 2]['Speed_kph'].mean()\n",
    "        s3_speed = lap_data[lap_data['Sector'] == 3]['Speed_kph'].mean()\n",
    "        if pd.notna(s1_speed) and pd.notna(s2_speed) and pd.notna(s3_speed):\n",
    "            sector_performance.append({'lap': lap_num,'sector_1_avg_speed': s1_speed,'sector_2_avg_speed': s2_speed,'sector_3_avg_speed': s3_speed,'overall_avg_speed': (s1_speed + s2_speed + s3_speed) / 3})\n",
    "\n",
    "    if len(sector_performance) == 0:\n",
    "        print(\"ERROR: No valid sector performance data could be calculated.\")\n",
    "        return None, None\n",
    "\n",
    "    sector_perf_df = pd.DataFrame(sector_performance)\n",
    "\n",
    "    # Ranked lists best->worst\n",
    "    s1_laps_ranked = sector_perf_df.sort_values('sector_1_avg_speed', ascending=False)['lap'].tolist()\n",
    "    s2_laps_ranked = sector_perf_df.sort_values('sector_2_avg_speed', ascending=False)['lap'].tolist()\n",
    "    s3_laps_ranked = sector_perf_df.sort_values('sector_3_avg_speed', ascending=False)['lap'].tolist()\n",
    "\n",
    "    # Define sector integer index ranges\n",
    "    s1_start = int(0)\n",
    "    s1_end = int(SECTOR_1_END_INDEX)\n",
    "    s2_start = int(SECTOR_1_END_INDEX) + 1\n",
    "    s2_end = int(SECTOR_2_END_INDEX)\n",
    "    s3_start = int(SECTOR_2_END_INDEX) + 1\n",
    "    s3_end = int(MAX_TRACK_INDEX)\n",
    "\n",
    "    s1_data = build_sector_segment(df, 1, s1_start, s1_end, s1_laps_ranked, max_interp_gap=max_interp_gap)\n",
    "    s2_data = build_sector_segment(df, 2, s2_start, s2_end, s2_laps_ranked, max_interp_gap=max_interp_gap)\n",
    "    s3_data = build_sector_segment(df, 3, s3_start, s3_end, s3_laps_ranked, max_interp_gap=max_interp_gap)\n",
    "\n",
    "    ideal_line_parts = []\n",
    "    if not s1_data.empty: ideal_line_parts.append(s1_data)\n",
    "    if not s2_data.empty: ideal_line_parts.append(s2_data)\n",
    "    if not s3_data.empty: ideal_line_parts.append(s3_data)\n",
    "\n",
    "    if not ideal_line_parts:\n",
    "        print(\"No sector data available after attempts to fill gaps.\")\n",
    "        return None, sector_perf_df\n",
    "\n",
    "    ideal_df = pd.concat(ideal_line_parts, ignore_index=True)\n",
    "    ideal_df = ideal_df.sort_values(by='Normalized_Index').reset_index(drop=True)\n",
    "\n",
    "    # Assign SourceLap if missing\n",
    "    if 'SourceLap' not in ideal_df.columns:\n",
    "        ideal_df['SourceLap'] = np.nan\n",
    "\n",
    "    # Smooth per-sector (safe) -- reuse earlier logic to avoid wrap-around artifacts\n",
    "    if len(ideal_df) < 4:\n",
    "        SMOOTHING_WINDOW_FINAL = 10\n",
    "        ideal_df['X_smooth'] = ideal_df['X'].rolling(window=SMOOTHING_WINDOW_FINAL, center=False, min_periods=1).mean()\n",
    "        ideal_df['Y_smooth'] = ideal_df['Y'].rolling(window=SMOOTHING_WINDOW_FINAL, center=False, min_periods=1).mean()\n",
    "    else:\n",
    "        try:\n",
    "            pts = ideal_df[['X', 'Y']].values.T\n",
    "            tck, u = splprep(pts, s=smoothness_param, k=3, per=False)\n",
    "            x_smooth, y_smooth = splev(u, tck)\n",
    "            ideal_df['X_smooth'] = x_smooth\n",
    "            ideal_df['Y_smooth'] = y_smooth\n",
    "        except Exception as e:\n",
    "            print(f\"Global spline failed ({e}), falling back to per-sector smoothing.\")\n",
    "            ideal_df['X_smooth'] = np.nan\n",
    "            ideal_df['Y_smooth'] = np.nan\n",
    "            for sector_num in [1, 2, 3]:\n",
    "                sdata = ideal_df[ideal_df['Sector'] == sector_num]\n",
    "                if len(sdata) >= 4:\n",
    "                    try:\n",
    "                        pts = sdata[['X', 'Y']].values.T\n",
    "                        tck_s, u_s = splprep(pts, s=max(0.1, smoothness_param / 10.0), k=3, per=False)\n",
    "                        xs, ys = splev(u_s, tck_s)\n",
    "                        ideal_df.loc[sdata.index, 'X_smooth'] = xs\n",
    "                        ideal_df.loc[sdata.index, 'Y_smooth'] = ys\n",
    "                    except Exception:\n",
    "                        ideal_df.loc[sdata.index, 'X_smooth'] = sdata['X'].rolling(window=2, min_periods=1).mean()\n",
    "                        ideal_df.loc[sdata.index, 'Y_smooth'] = sdata['Y'].rolling(window=2, min_periods=1).mean()\n",
    "                else:\n",
    "                    ideal_df.loc[sdata.index, 'X_smooth'] = sdata['X']\n",
    "                    ideal_df.loc[sdata.index, 'Y_smooth'] = sdata['Y']\n",
    "\n",
    "    return ideal_df, sector_perf_df\n",
    "\n",
    "\n",
    "# Run the v2 calculation and plot using existing plot_ideal_line if available\n",
    "try:\n",
    "    result_v2 = calculate_ideal_line_by_sectors_v2(full_track_df)\n",
    "    if result_v2 is not None:\n",
    "        ideal_path_df_v2, sector_perf_df_v2 = result_v2\n",
    "        # prefer existing plot_ideal_line if defined\n",
    "        if 'plot_ideal_line' in globals():\n",
    "            plot_ideal_line(ideal_path_df_v2, sector_perf_df_v2, full_track_df, vehicle_id_to_map)\n",
    "        else:\n",
    "            # simple fallback plotting\n",
    "            fig, ax = plt.subplots(figsize=(10, 7))\n",
    "            ax.plot(full_track_df['X'], full_track_df['Y'], color='lightgray', linewidth=1, alpha=0.4)\n",
    "            sector_colors = {1: 'red', 2: 'blue', 3: 'green'}\n",
    "            for n in [1,2,3]:\n",
    "                s = ideal_path_df_v2[ideal_path_df_v2['Sector'] == n]\n",
    "                if not s.empty:\n",
    "                    ax.plot(s['X_smooth'], s['Y_smooth'], color=sector_colors[n], linewidth=3)\n",
    "            ax.set_aspect('equal', adjustable='box')\n",
    "            plt.show()\n",
    "    else:\n",
    "        print('v2 calculation returned None')\n",
    "except Exception as e_run:\n",
    "    print('Failed to run v2 automatically:', e_run)\n"
   ]
  },
