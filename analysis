    "# Smooth ideal_df #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "ca64abdb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def smooth_ideal_df(ideal_df, smoothness_param=50, per_sector_fallback=True, blend_width=5,\n",
    "                    upsample_factor=None, method_hint=None):\n",
    "    \"\"\"\n",
    "    Backwards-compatible wrapper used by parameter sweep code.\n",
    "\n",
    "    Parameters\n",
    "    ----------\n",
    "    ideal_df : pd.DataFrame\n",
    "        Assembled ideal DataFrame (must contain X, Y, Sector, Normalized_Index ideally).\n",
    "    smoothness_param : numeric\n",
    "        Grid/sweep parameter (e.g. 1,10,50,200,1000). Mapped internally to upsample/window.\n",
    "    per_sector_fallback : bool\n",
    "        Kept for API compatibility; not used directly here.\n",
    "    blend_width : int\n",
    "        Number of points to blend at sector seams (passed to smoother).\n",
    "    upsample_factor : int or None\n",
    "        Optional override for resampling factor used by the non-spline smoother.\n",
    "    method_hint : str or None\n",
    "        Optional override for smoothing method: 'savgol' or 'rolling'.\n",
    "\n",
    "    Returns\n",
    "    -------\n",
    "    pd.DataFrame\n",
    "        DataFrame containing at least columns ['X_smooth', 'Y_smooth', 'Sector'].\n",
    "    \"\"\"\n",
    "    # Quick defensives\n",
    "    if ideal_df is None or ideal_df.empty:\n",
    "        return pd.DataFrame(columns=['X_smooth', 'Y_smooth', 'Sector'])\n",
    "\n",
    "    # Map smoothness_param into parameters for smooth_ideal_no_spline\n",
    "    if upsample_factor is None:\n",
    "        # coarse mapping: larger smoothness_param -> larger upsample (smoother-appearing)\n",
    "        upsample_factor = max(2, int(max(2, smoothness_param / 100)))\n",
    "\n",
    "    if method_hint is None:\n",
    "        method = 'savgol'\n",
    "    else:\n",
    "        method = method_hint\n",
    "\n",
    "    if smoothness_param >= 1000:\n",
    "        window = 15\n",
    "    elif smoothness_param >= 200:\n",
    "        window = 11\n",
    "    elif smoothness_param >= 50:\n",
    "        window = 7\n",
    "    else:\n",
    "        window = 5\n",
    "    polyorder = 2\n",
    "\n",
    "    # Prefer calling the robust non-spline smoother if available\n",
    "    if 'smooth_ideal_no_spline' in globals() and callable(globals()['smooth_ideal_no_spline']):\n",
    "        try:\n",
    "            out = globals()['smooth_ideal_no_spline'](\n",
    "                ideal_df,\n",
    "                upsample_factor=upsample_factor,\n",
    "                method=method,\n",
    "                window=window,\n",
    "                polyorder=polyorder,\n",
    "                blend=blend_width\n",
    "            )\n",
    "        except Exception as ex:\n",
    "            # fallback to a conservative rolling smooth on original points\n",
    "            tmp = ideal_df.copy()\n",
    "            tmp['X_smooth'] = tmp['X'].rolling(window=3, center=True, min_periods=1).mean()\n",
    "            tmp['Y_smooth'] = tmp['Y'].rolling(window=3, center=True, min_periods=1).mean()\n",
    "            return tmp[['X_smooth', 'Y_smooth', 'Sector']].reset_index(drop=True)\n",
    "    else:\n",
    "        # smooth_ideal_no_spline not present: do a conservative rolling fallback\n",
    "        tmp = ideal_df.copy()\n",
    "        tmp['X_smooth'] = tmp['X'].rolling(window=3, center=True, min_periods=1).mean()\n",
    "        tmp['Y_smooth'] = tmp['Y'].rolling(window=3, center=True, min_periods=1).mean()\n",
    "        return tmp[['X_smooth', 'Y_smooth', 'Sector']].reset_index(drop=True)\n",
    "\n",
    "    # smooth_ideal_no_spline returns resampled DataFrame; normalize its column names to match expectations\n",
    "    if 'X_final' in out.columns and 'Y_final' in out.columns:\n",
    "        out = out.rename(columns={'X_final': 'X_smooth', 'Y_final': 'Y_smooth'})\n",
    "\n",
    "    # ensure Sector exists\n",
    "    if 'Sector' not in out.columns:\n",
    "        out['Sector'] = 1\n",
    "\n",
    "    return out[['X_smooth', 'Y_smooth', 'Sector']].reset_index(drop=True)"
   ]
  },
