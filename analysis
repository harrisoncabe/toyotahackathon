    "# Smoothing parameter grid test #\n",
    "# Tries several smoothness_param values for v2 assembly and reports seam gaps before/after smoothing.\n",
    "\n",
    "params = [1, 10, 50, 200, 1000]\n",
    "results = []\n",
    "for sp in params:\n",
    "    print(f'--- testing smoothness_param={sp} ---')\n",
    "    res = calculate_ideal_line_by_sectors_v2(full_track_df, smoothness_param=sp)\n",
    "    if res is None:\n",
    "        print('v2 returned None for sp', sp)\n",
    "        continue\n",
    "    ideal_raw, sector_perf = res\n",
    "\n",
    "    def seam_gaps(d):\n",
    "        gaps = {}\n",
    "        for a,b in [(1,2),(2,3)]:\n",
    "            sa = d[d['Sector']==a]\n",
    "            sb = d[d['Sector']==b]\n",
    "            if not sa.empty and not sb.empty:\n",
    "                try:\n",
    "                    gaps[f\"{a}-{b}\"] = float(np.linalg.norm(sa[['X','Y']].iloc[-1].values - sb[['X','Y']].iloc[0].values))\n",
    "                except Exception:\n",
    "                    gaps[f\"{a}-{b}\"] = np.nan\n",
    "            else:\n",
    "                gaps[f\"{a}-{b}\"] = np.nan\n",
    "        return gaps\n",
    "\n",
    "    gaps_before = seam_gaps(ideal_raw)\n",
    "    try:\n",
    "        ideal_sm = smooth_ideal_no_spline(ideal_raw, smoothness_param=sp, per_sector_fallback=True, blend_width=5)\n",
    "    except Exception as e:\n",
    "        print('smooth_ideal_no_spline failed for sp', sp, e)\n",
    "        ideal_sm = ideal_raw.copy()\n",
    "\n",
    "    temp = ideal_sm.copy()\n",
    "    if 'X_smooth' in temp.columns and 'Y_smooth' in temp.columns:\n",
    "        temp['X'] = temp['X_smooth']\n",
    "        temp['Y'] = temp['Y_smooth']\n",
    "    gaps_after = seam_gaps(temp)\n",
    "    print(f'sp={sp} gaps_before={gaps_before} gaps_after={gaps_after}')\n",
    "\n",
    "    # compute gap_sum (sum of numeric gaps)\n",
    "    gap_vals = [v for v in gaps_after.values() if (v is not None and not (isinstance(v, float) and np.isnan(v)))]\n",
    "    gap_sum = float(np.sum(gap_vals)) if gap_vals else float('nan')\n",
    "\n",
    "    # compute mean distance from full track points to this candidate ideal (lower is better)\n",
    "    try:\n",
    "        if not temp.empty and 'X' in temp.columns and 'Y' in temp.columns and not full_track_df.empty:\n",
    "            nbr = NearestNeighbors(n_neighbors=1).fit(temp[['X','Y']].values)\n",
    "            dists, _ = nbr.kneighbors(full_track_df[['X','Y']].values)\n",
    "            mean_dist = float(dists.mean())\n",
    "        else:\n",
    "            mean_dist = float('nan')\n",
    "    except Exception as e:\n",
    "        print('mean distance calc failed for sp', sp, e)\n",
    "        mean_dist = float('nan')\n",
    "\n",
    "    # canonical score: primarily gap_sum, secondarily mean_dist (small is better)\n",
    "    if not np.isnan(gap_sum):\n",
    "        score = float(gap_sum) + 0.01 * (float(mean_dist) if not np.isnan(mean_dist) else 0.0)\n",
    "    else:\n",
    "        score = float('nan')\n",
    "\n",
    "    # append a dict so downstream summary can build a DataFrame reliably\n",
    "    r = {\n",
    "        'smoothness_param': int(sp),\n",
    "        'gap_1_2': gaps_after.get('1-2', np.nan),\n",
    "        'gap_2_3': gaps_after.get('2-3', np.nan),\n",
    "        'gap_sum': gap_sum,\n",
    "        'mean_dist_to_fulltrack': mean_dist,\n",
    "        'score': score,\n",
    "        'ideal_sm': ideal_sm  # keep the DataFrame itself for later inspection if needed\n",
    "    }\n",
    "    results.append(r)\n",
    "\n",
    "    stored_ideals = stored_ideals if 'stored_ideals' in globals() else {}\n",
    "    stored_ideals[sp] = ideal_sm\n",
    "# plot last smoothed result for visual check\n",
    "if 'ideal_sm' in locals():\n",
    "    fig, ax = plt.subplots(figsize=(10,6))\n",
    "    ax.plot(full_track_df['X'], full_track_df['Y'], color='lightgray', alpha=0.4)\n",
    "    for n,c in zip([1,2,3], ['red','blue','green']):\n",
    "        s = ideal_sm[ideal_sm['Sector']==n]\n",
    "        if not s.empty:\n",
    "            # prefer smoothed coords if present\n",
    "            if 'X_smooth' in s.columns and 'Y_smooth' in s.columns:\n",
    "                ax.plot(s['X_smooth'], s['Y_smooth'], color=c, linewidth=2, label=f'Sector {n}')\n",
    "            else:\n",
    "                ax.plot(s['X'], s['Y'], color=c, linewidth=2, label=f'Sector {n}')\n",
    "    ax.legend(); ax.set_aspect('equal'); plt.title('Example smoothed ideal (last param)'); plt.show()\n",
    "\n",
    "try:\n",
    "    # Build a DataFrame excluding heavy objects\n",
    "    summary_rows = []\n",
    "    for r in results:\n",
    "        if isinstance(r, dict):\n",
    "            row = {k: v for k, v in r.items() if k != 'ideal_sm'}\n",
    "        else:\n",
    "            # attempt to coerce tuples/lists to dicts if possible, else skip\n",
    "            try:\n",
    "                row = dict(r)\n",
    "                row.pop('ideal_sm', None)\n",
    "            except Exception:\n",
    "                # fallback: minimal empty row\n",
    "                row = {}\n",
    "        summary_rows.append(row)\n",
    "\n",
    "    summary_df = pd.DataFrame(summary_rows)\n",
    "\n",
    "    # Ensure score column exists and is numeric\n",
    "    if 'score' not in summary_df.columns:\n",
    "        summary_df['score'] = np.nan\n",
    "    summary_df['score'] = pd.to_numeric(summary_df['score'], errors='coerce')\n",
    "    # replace NaN scores with +inf so they sort to the end\n",
    "    summary_df['score'] = summary_df['score'].fillna(np.inf)\n",
    "\n",
    "    summary_df = summary_df.sort_values('score', na_position='last').reset_index(drop=True)\n",
    "    print('Sweep summary (best first):')\n",
    "    print(summary_df.to_string(index=False))\n",
    "except Exception as e:\n",
    "    print('Could not build summary DataFrame or sort by score:', e)\n"
   ]
