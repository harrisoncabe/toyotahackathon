    "# Wrap-aware stitching and seam blending helper (fixes gaps between sectos) #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c37c40fa",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Using assembled ideal variable: ideal_clean\n",
      "3-1 gap before (m): 25.872648\n",
      "3-1 gap after blend (m): 302.633954\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Wrap-aware stitching and seam blending helper\n",
    "# This cell detects the largest gap in the assembled ideal (typically between Sector 3 end and Sector 1 start),\n",
    "# rotates the sequence so the track order is continuous, and linearly blends `blend` points across the seam.\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "def wrap_stitch_and_blend(ideal_df, xcol_pref=('X_smooth','X'), ycol_pref=('Y_smooth','Y'), blend=10):\n",
    "    \"\"\"\n",
    "    Rotate `ideal_df` so the largest Euclidean gap between consecutive points becomes the cut point,\n",
    "    then blend `blend` points on either side to smooth the seam.\n",
    "\n",
    "    Parameters\n",
    "    ----------\n",
    "    ideal_df : pd.DataFrame\n",
    "        Assembled ideal path with X/Y or X_smooth/Y_smooth columns.\n",
    "    xcol_pref, ycol_pref : tuple\n",
    "        Preference order for x/y columns to use for geometry.\n",
    "    blend : int\n",
    "        Number of points to blend on each side of the seam. If 0, no blending is applied.\n",
    "\n",
    "    Returns\n",
    "    -------\n",
    "    out_df : pd.DataFrame\n",
    "        Copy of ideal_df rotated and with added columns 'X_final','Y_final'.\n",
    "    info : dict\n",
    "        Diagnostic information including original max gap and post-blend gap.\n",
    "    \"\"\"\n",
    "    df = ideal_df.copy().reset_index(drop=True)\n",
    "\n",
    "    # choose columns\n",
    "    for xc in xcol_pref:\n",
    "        if xc in df.columns:\n",
    "            xcol = xc\n",
    "            break\n",
    "    else:\n",
    "        xcol = xcol_pref[-1]\n",
    "    for yc in ycol_pref:\n",
    "        if yc in df.columns:\n",
    "            ycol = yc\n",
    "            break\n",
    "    else:\n",
    "        ycol = ycol_pref[-1]\n",
    "\n",
    "    xs = df[xcol].astype(float).to_numpy()\n",
    "    ys = df[ycol].astype(float).to_numpy()\n",
    "\n",
    "    if len(xs) < 2:\n",
    "        df['X_final'] = xs\n",
    "        df['Y_final'] = ys\n",
    "        return df, {'max_gap': 0.0, 'post_gap': 0.0}\n",
    "\n",
    "    # compute consecutive gaps including wrap (last->first)\n",
    "    pts = np.vstack([xs, ys]).T\n",
    "    diffs = pts[1:] - pts[:-1]\n",
    "    dists = np.sqrt((diffs**2).sum(axis=1))\n",
    "    # include wrap gap\n",
    "    wrap_vec = pts[0] - pts[-1]\n",
    "    wrap_dist = np.sqrt((wrap_vec**2).sum())\n",
    "    all_gaps = np.concatenate([dists, [wrap_dist]])\n",
    "\n",
    "    max_gap_idx = np.argmax(all_gaps)\n",
    "    max_gap = float(all_gaps[max_gap_idx])\n",
    "\n",
    "    # If max gap is last element, that indicates wrap between last and first\n",
    "    if max_gap_idx == len(dists):\n",
    "        cut_index = 0  # already continuous if you start at 0; but we prefer to rotate so seam at end\n",
    "        # rotate so that the point AFTER the gap (index 0) becomes start (no-op), but we'll cut at 0\n",
    "        rotate_at = 0\n",
    "    else:\n",
    "        # gap between element max_gap_idx and max_gap_idx+1 -> we want to start at max_gap_idx+1\n",
    "        rotate_at = max_gap_idx + 1\n",
    "\n",
    "    # rotate dataframe\n",
    "    if rotate_at == 0:\n",
    "        rotated = df.copy().reset_index(drop=True)\n",
    "    else:\n",
    "        rotated = pd.concat([df.iloc[rotate_at:].reset_index(drop=True), df.iloc[:rotate_at].reset_index(drop=True)], ignore_index=True)\n",
    "\n",
    "    # prepare final coords (start from chosen geometry)\n",
    "    rx = rotated[xcol].astype(float).to_numpy()\n",
    "    ry = rotated[ycol].astype(float).to_numpy()\n",
    "\n",
    "    # blending across seam: blend last `blend` points with first `blend` points\n",
    "    X_final = rx.copy()\n",
    "    Y_final = ry.copy()\n",
    "    n = len(X_final)\n",
    "    b = int(blend)\n",
    "    if b > 0 and n > 2*b:\n",
    "        for i in range(b):\n",
    "            alpha = (i + 1) / (b + 1)  # small -> near A, large -> near B\n",
    "            ai = n - b + i  # index in A region\n",
    "            bi = i  # index in B region\n",
    "            Xa = X_final[ai]\n",
    "            Ya = Y_final[ai]\n",
    "            Xb = X_final[bi]\n",
    "            Yb = Y_final[bi]\n",
    "            Xm = (1 - alpha) * Xa + alpha * Xb\n",
    "            Ym = (1 - alpha) * Ya + alpha * Yb\n",
    "            X_final[ai] = Xm\n",
    "            Y_final[ai] = Ym\n",
    "            X_final[bi] = Xm\n",
    "            Y_final[bi] = Ym\n",
    "    else:\n",
    "        # if blending not possible, keep original\n",
    "        pass\n",
    "\n",
    "    rotated['X_final'] = X_final\n",
    "    rotated['Y_final'] = Y_final\n",
    "\n",
    "    # compute post-blend max gap for diagnostics\n",
    "    pts_f = np.vstack([rotated['X_final'].to_numpy(), rotated['Y_final'].to_numpy()]).T\n",
    "    diffs_f = pts_f[1:] - pts_f[:-1]\n",
    "    dists_f = np.sqrt((diffs_f**2).sum(axis=1))\n",
    "    wrap_vec_f = pts_f[0] - pts_f[-1]\n",
    "    wrap_dist_f = np.sqrt((wrap_vec_f**2).sum())\n",
    "    post_max_gap = float(max(np.max(dists_f) if len(dists_f)>0 else 0.0, wrap_dist_f))\n",
    "\n",
    "    info = {'max_gap': max_gap, 'post_gap': post_max_gap, 'cut_index_before_rotate': max_gap_idx, 'rotate_at': rotate_at}\n",
    "\n",
    "    return rotated, info\n",
    "\n",
    "\n",
    "# Example: run on the assembled ideal currently in kernel and show numeric improvement, can be helpful to show differences in blend level\n",
    "try:\n",
    "    # find assembled variable (common names)\n",
    "    names = ['ideal_clean','ideal_sm','ideal_top','ideal_path_df_v2','ideal_path_df','ideal_raw','ideal_path_df','ideal_df']\n",
    "    varname = None\n",
    "    for n in names:\n",
    "        if n in globals():\n",
    "            varname = n\n",
    "            break\n",
    "    if varname is None:\n",
    "        print('No assembled ideal variable found in kernel. Build the ideal first and re-run this cell.')\n",
    "    else:\n",
    "        print('Using assembled ideal variable:', varname)\n",
    "        assembled = globals()[varname].copy()\n",
    "        # choose columns for gap calc\n",
    "        xcol = 'X_smooth' if 'X_smooth' in assembled.columns else 'X'\n",
    "        ycol = 'Y_smooth' if 'Y_smooth' in assembled.columns else 'Y'\n",
    "        def compute_wrap_gap(df, xcol, ycol):\n",
    "            pts = df[[xcol,ycol]].dropna().to_numpy()\n",
    "            if len(pts) < 2:\n",
    "                return 0.0\n",
    "            diffs = pts[1:] - pts[:-1]\n",
    "            dists = np.sqrt((diffs**2).sum(axis=1))\n",
    "            wrap_vec = pts[0] - pts[-1]\n",
    "            wrap_dist = np.sqrt((wrap_vec**2).sum())\n",
    "            return float(wrap_dist)\n",
    "\n",
    "        before_gap = compute_wrap_gap(assembled, xcol, ycol)\n",
    "        print(f'3-1 gap before (m): {before_gap:.6f}')\n",
    "        fixed, info = wrap_stitch_and_blend(assembled, blend=12)\n",
    "        after_gap = info['post_gap']\n",
    "        print(f'3-1 gap after blend (m): {after_gap:.6f}')\n",
    "\n",
    "        # quick plot\n",
    "        plt.figure(figsize=(10,6))\n",
    "        plt.plot(full_track_df['X'], full_track_df['Y'], color='lightgray', alpha=0.4, label='All laps')\n",
    "        plt.plot(assembled[xcol], assembled[ycol], color='gray', linewidth=2, label='assembled (pre)')\n",
    "        plt.plot(fixed['X_final'], fixed['Y_final'], color='red', linewidth=2, label=f'wrapped+blend (post)')\n",
    "        plt.axis('equal')\n",
    "        plt.legend()\n",
    "        plt.title(f'Wrap-aware stitch: gap {before_gap:.1f} -> {after_gap:.1f} m')\n",
    "        plt.show()\n",
    "except Exception as e:\n",
    "    print('Wrap-aware stitch cell failed:', e)\n"
   ]
  },
