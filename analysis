   "source": [
    "df15 = pd.read_csv(\"barber/R1_barber_telemetry_data.csv\", sep = ',')\n",
    "\n",
    "start_finish_lat = 33.532320\n",
    "start_finish_lon = -86.619550\n",
    "pit_entry_lat = 33.533200  \n",
    "pit_entry_lon = -86.616300\n",
    "pit_exit_lat = 33.531280 \n",
    "pit_exit_long = -86.619000\n",
    "\n",
    "distance_data = { \n",
    "    'Circuit_Center_Line_Length': 144672, 'Start_Line_Offset': 0.0, 'Sector_1_End': 40512, \n",
    "    'Sector_2_End': 102732, 'Sector_3_End': 144672.0, 'Pit_In_from_SF': 131296.0,\n",
    "    'Pit_Out_from_SFP': 5352.0, 'Pit_in_to_Pit_Out': 18794.0\n",
    "}\n",
    "INCH_TO_METER = 0.0254\n",
    "\n",
    "required_channels = [\n",
    "    'VBOX_Lat_Min', 'VBOX_Long_Minutes', 'Laptrigger_lapdist_dls', \n",
    "    'nmot', 'Steering_Angle', 'gear', 'pbrake_r', 'pbrake_f', 'accx_can', 'accy_can', 'aps',\n",
    "    'speed'\n",
    "]\n",
    "filtered_df = df15[df15['telemetry_name'].isin(required_channels)].copy()\n",
    "\n",
    "# Pivot the data into wide format\n",
    "telemetry_wide_df = filtered_df.pivot_table(\n",
    "    index=['timestamp', 'lap', 'vehicle_id', 'outing'], \n",
    "    columns='telemetry_name', \n",
    "    values='telemetry_value'\n",
    ").reset_index()\n",
    "\n",
    "# Rename columns and convert to numeric/datetime\n",
    "telemetry_wide_df = telemetry_wide_df.rename(columns={\n",
    "    'VBOX_Lat_Min': 'Latitude', 'VBOX_Long_Minutes': 'Longitude', 'Laptrigger_lapdist_dls': 'LapDist_in',\n",
    "    'nmot': 'RPM', 'Steering_Angle': 'SteeringAngle', 'pbrake_r': 'BrakeRear', 'pbrake_f': 'BrakeFront',\n",
    "    'accx_can': 'AccX', 'accy_can': 'AccY', 'aps': 'Throttle', 'gear': 'Gear',\n",
    "    'speed': 'Speed_kph'\n",
    "})\n",
    "\n",
    "# Convert required columns to numeric\n",
    "numeric_cols = ['Latitude', 'Longitude', 'LapDist_in', 'RPM', 'SteeringAngle', 'Gear', 'BrakeRear', 'BrakeFront', 'AccX', 'AccY', 'Throttle', 'Speed_kph']\n",
    "for col in numeric_cols:\n",
    "    telemetry_wide_df[col] = pd.to_numeric(telemetry_wide_df[col], errors='coerce')\n",
    "\n",
    "# --- Data Cleaning (Gear specific) ---\n",
    "telemetry_wide_df['Gear'] = telemetry_wide_df['Gear'].ffill().bfill()\n",
    "telemetry_wide_df['Gear'] = telemetry_wide_df['Gear'].round(0)\n",
    "telemetry_wide_df['Gear'] = telemetry_wide_df['Gear'].astype('Int64')\n",
    "\n",
    "# --- Define the global coordinate system once for the entire session ---\n",
    "telemetry_wide_df['vehicle_id'] = telemetry_wide_df['vehicle_id'].str.strip() \n",
    "min_lon = telemetry_wide_df['Longitude'].min()\n",
    "min_lat = telemetry_wide_df['Latitude'].min()\n",
    "scale_factor = 111320 # Approx meters per degree Lat/Lon near the equator\n",
    "\n",
    "telemetry_wide_df['X'] = (telemetry_wide_df['Longitude'] - min_lon) * scale_factor\n",
    "telemetry_wide_df['Y'] = (telemetry_wide_df['Latitude'] - min_lat) * scale_factor\n",
    "\n",
    "# --- Select ALL data for a specific vehicle to trace the entire track ---\n",
    "vehicle_id_to_map = 'GR86-006-7'\n",
    "full_track_df = telemetry_wide_df[\n",
    "    (telemetry_wide_df['vehicle_id'] == vehicle_id_to_map)\n",
    "].copy()\n",
    "\n",
    "\n",
    "if full_track_df.empty:\n",
    "    print(f\"\\nERROR: No data available for Vehicle {vehicle_id_to_map}.\")\n",
    "else:\n",
    "    print(f\"\\nTotal data points found for plotting: {len(full_track_df)}.\")\n",
    "    \n",
    "    # CRITICAL FIX: Ensure all X and Y coordinates are filled before sector selection\n",
    "    full_track_df = full_track_df.sort_values(by=['lap', 'timestamp']).copy()\n",
    "    full_track_df['X'] = full_track_df.groupby('lap')['X'].transform(lambda x: x.interpolate(method='linear').ffill().bfill())\n",
    "    full_track_df['Y'] = full_track_df.groupby('lap')['Y'].transform(lambda x: x.interpolate(method='linear').ffill().bfill())\n",
    "\n",
    "    # Now you can drop rows where Lat/Long might still be missing entirely (if any exist)\n",
    "    full_track_df.dropna(subset=['Latitude', 'Longitude', 'Gear', 'Speed_kph', 'X', 'Y'], inplace=True)\n",
    "\n",
    "    print(\"\\nGenerating a stable center line for data normalization...\")\n",
    "\n",
    "    # 1. Create the authoritative reference line: Average all X,Y points into a single line\n",
    "    # We must ensure 'LapDist_in' is usable here. \n",
    "    # Use the interpolation logic you already have inside the 'calculate_ideal_line_by_sectors' function first\n",
    "    \n",
    "    # Select one representative lap (e.g., Lap 12, or your average lap) that you know is complete and clean.\n",
    "    # We'll call this 'reference_lap'\n",
    "    reference_lap_num = 24 # <--- CHANGE THIS LAP NUMBER if lap 12 is an outlier/pit lap\n",
    "    reference_lap_df = full_track_df[full_track_df['lap'] == reference_lap_num].copy()\n",
    "\n",
    "    # Ensure the reference lap is sorted sequentially\n",
    "    SMOOTHING_WINDOW_TEMPLATE = 2                                                                                                                           # Small window for initial smoothing\n",
    "    \n",
    "    center_line_points = reference_lap_df[['X', 'Y']].rolling(\n",
    "        window=SMOOTHING_WINDOW_TEMPLATE, \n",
    "        center=False, \n",
    "        min_periods=1\n",
    "    ).mean().dropna().values\n",
    "    \n",
    "    # Ensure center_line_points is contiguous and doesn't have NAs\n",
    "    if len(center_line_points) == 0:\n",
    "        raise ValueError(f\"Reference lap {reference_lap_num} is empty or too short to smooth.\")\n",
    "\n",
    "    \n",
    "    # Optional: Calculate the cumulative distance along this new, smooth center line\n",
    "    # This requires more complex math (haversine or Euclidean distance accumulation) which I won't write here in full.\n",
    "    # We will rely on the *indices* of the reference line for distance proxy.\n",
    "    \n",
    "    # 2. Fit the Nearest Neighbors model on this new, clean center line\n",
    "    nbrs = NearestNeighbors(n_neighbors=1, algorithm='ball_tree').fit(center_line_points)\n",
    "\n",
    "    # 3. Apply the snapping to ALL telemetry data points (full_track_df)\n",
    "    all_telemetry_points = full_track_df[['X', 'Y']].values\n",
    "    distances_to_line, indices_on_line = nbrs.kneighbors(all_telemetry_points)\n",
    "\n",
    "    # 4. CRITICAL: Use the index along the center_line_points as a PROXY for distance\n",
    "    # We are effectively sorting the full_track_df by its position along the reference line.\n",
    "    full_track_df['Normalized_Index'] = indices_on_line.flatten()\n",
    "    \n",
    "    print(f\"Average distance from telemetry points to center line: {np.mean(distances_to_line):.2f} meters.\")\n",
    "    \n",
    "    # Now, whenever you analyze sectors or plots, sort by 'Normalized_Index' instead of 'LapDist_in' or 'Time'.\n",
    "\n",
    "    # --- Calculate additional metrics ---\n",
    "    full_track_df['BrakeIntensity'] = full_track_df['BrakeRear'] + full_track_df['BrakeFront']\n",
    "    full_track_df['BrakeIntensity'] = full_track_df['BrakeIntensity'].clip(upper=100) \n",
    "    G_FORCE = 9.81\n",
    "    full_track_df['G_Long'] = full_track_df['AccX'] / G_FORCE\n",
    "    \n",
    "    full_track_df['Time'] = pd.to_datetime(full_track_df['timestamp'])\n",
    "    full_track_df.sort_values(by='Time', inplace=True) \n",
    "        # --- 2. Calculate Lap Durations from Event Log and Plot Outliers ---\n",
    "    \n",
    "    try:\n",
    "        # Assuming this is the path to the file with the 'timestamp' and 'lap' columns\n",
    "        lap_times_df = pd.read_csv(\"barber/R1_barber_lap_time.csv\") \n",
    "\n",
    "        driver_events = lap_times_df[lap_times_df['vehicle_id'] == vehicle_id_to_map].copy()\n",
    "        \n",
    "        # Ensure timestamps are datetime objects and sort them\n",
    "        driver_events['timestamp'] = pd.to_datetime(driver_events['timestamp'])\n",
    "        driver_events.sort_values(by='timestamp', inplace=True)\n",
    "\n",
    "        # Calculate the difference between consecutive timestamps to get lap duration\n",
    "        # Shift the timestamps down one row (the previous lap's end time)\n",
    "        driver_events['previous_timestamp'] = driver_events.groupby('outing')['timestamp'].shift(1)\n",
    "        \n",
    "        # Calculate the duration for the lap just completed\n",
    "        driver_events['time_delta'] = driver_events['timestamp'] - driver_events['previous_timestamp']\n",
    "        \n",
    "        # Convert Timedelta to total seconds (float)\n",
    "        driver_events['time_seconds'] = driver_events['time_delta'].dt.total_seconds()\n",
    "\n",
    "        # Keep only valid lap data (drop the very first lap of an outing, which has no previous timestamp)\n",
    "        driver_laps = driver_events.dropna(subset=['time_seconds']).copy()\n",
    "        \n",
    "        # Filter out unusually slow laps (like formation laps or extremely long pit stops if they mess up analysis)\n",
    "        driver_laps = driver_laps[driver_laps['time_seconds'] > 60] # e.g., only laps longer than 60s\n",
    "\n",
    "        \n",
    "        # Calculate statistical properties (IQR method for outliers)\n",
    "        Q1 = driver_laps['time_seconds'].quantile(0.25)\n",
    "        Q3 = driver_laps['time_seconds'].quantile(0.75)\n",
    "        IQR = Q3 - Q1\n",
    "        lower_bound = Q1 - 1.5 * IQR\n",
    "        upper_bound = Q3 + 1.5 * IQR\n",
    "\n",
    "        driver_laps['is_outlier'] = (driver_laps['time_seconds'] < lower_bound) | (driver_laps['time_seconds'] > upper_bound)\n",
    "        \n",
    "        # Plotting the lap times with outliers highlighted\n",
    "        plt.figure(figsize=(10, 6))\n",
    "        \n",
    "        # Plot non-outliers first\n",
    "        plt.bar(driver_laps[~driver_laps['is_outlier']]['lap'], \n",
    "                driver_laps[~driver_laps['is_outlier']]['time_seconds'], \n",
    "                color='skyblue', label='Valid Lap Times')\n",
    "        \n",
    "        # Plot outliers\n",
    "        plt.bar(driver_laps[driver_laps['is_outlier']]['lap'], \n",
    "                driver_laps[driver_laps['is_outlier']]['time_seconds'], \n",
    "                color='red', label='Outlier Lap (e.g., Pit Lap)')\n",
    "\n",
    "        # Draw a horizontal line for average lap time (excluding outliers for a 'valid pace' average)\n",
    "        average_valid_lap = driver_laps[~driver_laps['is_outlier']]['time_seconds'].mean()\n",
    "        plt.axhline(y=average_valid_lap, color='gray', linestyle='--', label=f'Avg Valid Lap: {average_valid_lap:.2f}s')\n",
    "\n",
    "\n",
    "        plt.title(f'Calculated Lap Times and Outliers for Vehicle {vehicle_id_to_map}')\n",
    "        plt.xlabel('Lap Number')\n",
    "        plt.ylabel('Lap Time (Seconds)')\n",
    "        plt.legend()\n",
    "        plt.grid(axis='y')\n",
    "        plt.show()\n",
    "\n",
    "    except FileNotFoundError:\n",
    "        print(\"\\nCould not find lap event CSV. Skipping lap time chart.\")\n",
    "        lap_events_df = None\n",
    "\n",
    "    # --- 3. Visualize the Ideal Line Based on Best Sector Performance ---\n",
    "\n",
    "    def calculate_ideal_line_by_sectors(df):\n",
    "        \"\"\"\n",
    "        Calculate ideal line by finding the lap with the best average speed through each sector,\n",
    "        then combining those segments into a theoretical best lap.\n",
    "        \"\"\"\n",
    "        df = df.copy()\n",
    "        df = df.sort_values(by=['lap', 'Normalized_Index']).copy()\n",
    "\n",
    "        # --- DYNAMICALLY DETERMINE INDEX BOUNDARIES (using physical distances) ---\n",
    "        TARGET_S1_DIST_IN = 40512\n",
    "        TARGET_S2_DIST_IN = 102732\n",
    "        \n",
    "        s1_boundary_points = df[(df['LapDist_in'].notna()) & (df['LapDist_in'] >= TARGET_S1_DIST_IN - 500) & (df['LapDist_in'] <= TARGET_S1_DIST_IN + 500)]\n",
    "        s2_boundary_points = df[(df['LapDist_in'].notna()) & (df['LapDist_in'] >= TARGET_S2_DIST_IN - 500) & (df['LapDist_in'] <= TARGET_S2_DIST_IN + 500)]\n",
    "\n",
    "        SECTOR_1_END_INDEX = int(round(s1_boundary_points['Normalized_Index'].median())) if not s1_boundary_points.empty else df['Normalized_Index'].max() / 3\n",
    "        SECTOR_2_END_INDEX = int(round(s2_boundary_points['Normalized_Index'].median())) if not s2_boundary_points.empty else (df['Normalized_Index'].max() / 3) * 2\n",
    "        MAX_TRACK_INDEX = df['Normalized_Index'].max()\n",
    "        \n",
    "        print(f\"\\nSector boundaries calculated.\")\n",
    "        \n",
    "        def assign_sector(normalized_index):\n",
    "            if pd.isna(normalized_index): return None\n",
    "            if normalized_index <= SECTOR_1_END_INDEX: return 1\n",
    "            elif normalized_index <= SECTOR_2_END_INDEX: return 2\n",
    "            else: return 3\n",
    "        \n",
    "        df['Sector'] = df['Normalized_Index'].apply(assign_sector)\n",
    "        \n",
    "        # --- Sector Performance Calculation ---\n",
    "        sector_performance = []\n",
    "        \n",
    "        for lap_num in sorted(df['lap'].unique()):\n",
    "            lap_data = df[df['lap'] == lap_num].copy()\n",
    "            \n",
    "            # Check if lap has all three sectors\n",
    "            sectors_in_lap = lap_data['Sector'].nunique()\n",
    "            if sectors_in_lap < 3:\n",
    "                continue\n",
    "            \n",
    "            # Calculate average speed for each sector\n",
    "            s1_speed = lap_data[lap_data['Sector'] == 1]['Speed_kph'].mean()\n",
    "            s2_speed = lap_data[lap_data['Sector'] == 2]['Speed_kph'].mean()\n",
    "            s3_speed = lap_data[lap_data['Sector'] == 3]['Speed_kph'].mean()\n",
    "            \n",
    "            # Only include laps with valid speed data\n",
    "            if pd.notna(s1_speed) and pd.notna(s2_speed) and pd.notna(s3_speed):\n",
    "                sector_performance.append({\n",
    "                    'lap': lap_num,\n",
    "                    'sector_1_avg_speed': s1_speed,\n",
    "                    'sector_2_avg_speed': s2_speed,\n",
    "                    'sector_3_avg_speed': s3_speed,\n",
    "                    'overall_avg_speed': (s1_speed + s2_speed + s3_speed) / 3\n",
    "                })\n",
    "        \n",
    "        if len(sector_performance) == 0:\n",
    "            print(\"ERROR: No valid sector performance data could be calculated.\")\n",
    "            return None, None\n",
    "        \n",
    "        sector_perf_df = pd.DataFrame(sector_performance)\n",
    "        \n",
    "        # Find the lap with the best (highest) average speed for each sector\n",
    "        best_s1_lap = sector_perf_df.loc[sector_perf_df['sector_1_avg_speed'].idxmax(), 'lap']\n",
    "        best_s2_lap = sector_perf_df.loc[sector_perf_df['sector_2_avg_speed'].idxmax(), 'lap']\n",
    "        best_s3_lap = sector_perf_df.loc[sector_perf_df['sector_3_avg_speed'].idxmax(), 'lap']\n",
    "        \n",
    "        print(f\"\\nBest Sector Performance (by average speed):\")\n",
    "        print(f\"  Sector 1: Lap {best_s1_lap}\")\n",
    "        print(f\"  Sector 2: Lap {best_s2_lap}\")\n",
    "        print(f\"  Sector 3: Lap {best_s3_lap}\")\n",
    "        \n",
    "        # Extract the path data from the best sectors\n",
    "        ideal_line_parts = []\n",
    "        \n",
    "        s1_data = df[(df['lap'] == best_s1_lap) & (df['Sector'] == 1)].copy()\n",
    "        s2_data = df[(df['lap'] == best_s2_lap) & (df['Sector'] == 2)].copy()\n",
    "        s3_data = df[(df['lap'] == best_s3_lap) & (df['Sector'] == 3)].copy()\n",
    "\n",
    "        if not s1_data.empty: ideal_line_parts.append(s1_data)\n",
    "        if not s2_data.empty: ideal_line_parts.append(s2_data)\n",
    "        if not s3_data.empty: ideal_line_parts.append(s3_data)\n",
    "        \n",
    "        # Combine all sectors\n",
    "        ideal_df = pd.concat(ideal_line_parts, ignore_index=True)\n",
    "        # We sort here using the NEW Normalized_Index\n",
    "        ideal_df = ideal_df.sort_values(by='Normalized_Index').reset_index(drop=True)\n",
    "        \n",
    "        # Store which lap each sector came from for visualization\n",
    "        ideal_df['SourceLap'] = ideal_df.apply(lambda row: \n",
    "            best_s1_lap if row['Sector'] == 1 else \n",
    "            (best_s2_lap if row['Sector'] == 2 else best_s3_lap), axis=1)\n",
    "\n",
    "        points = ideal_df[['X', 'Y']].values.T \n",
    "        \n",
    "        # 's' is the smoothness parameter (lower = follows points closely, higher = smoother line)\n",
    "        # 'per=True' enforces cyclicity (treats the start and end as connected)\n",
    "        smoothness_param = 100\n",
    "\n",
    "        if len(ideal_df) < 4:\n",
    "            SMOOTHING_WINDOW_FINAL = 2\n",
    "            ideal_df['X_smooth'] = ideal_df['X'].rolling(window=SMOOTHING_WINDOW_FINAL, center=False, min_periods=1).mean()\n",
    "            ideal_df['Y_smooth'] = ideal_df['Y'].rolling(window=SMOOTHING_WINDOW_FINAL, center=False, min_periods=1).mean()\n",
    "        else:\n",
    "            # Try a global non-periodic spline first (prevents wrap-around smoothing)\n",
    "            try:\n",
    "                points = ideal_df[['X', 'Y']].values.T\n",
    "                tck, u = splprep(points, s=smoothness_param, k=3, per=False)   # <-- per=False (important)\n",
    "                x_smooth, y_smooth = splev(u, tck)\n",
    "                ideal_df['X_smooth'] = x_smooth\n",
    "                ideal_df['Y_smooth'] = y_smooth\n",
    "            except Exception as e:\n",
    "                # If that fails (e.g., numeric issues), fall back to per-sector smoothing\n",
    "                print(f\"Global spline failed ({e}), falling back to per-sector smoothing.\")\n",
    "                ideal_df['X_smooth'] = np.nan\n",
    "                ideal_df['Y_smooth'] = np.nan\n",
    "\n",
    "                for sector_num in [1, 2, 3]:\n",
    "                    sdata = ideal_df[ideal_df['Sector'] == sector_num]\n",
    "                    if len(sdata) >= 4:\n",
    "                        try:\n",
    "                            pts = sdata[['X', 'Y']].values.T\n",
    "                            # Use reduced smoothness for smaller segments\n",
    "                            tck_s, u_s = splprep(pts, s=max(0.1, smoothness_param / 10.0), k=3, per=False)\n",
    "                            xs, ys = splev(u_s, tck_s)\n",
    "                            ideal_df.loc[sdata.index, 'X_smooth'] = xs\n",
    "                            ideal_df.loc[sdata.index, 'Y_smooth'] = ys\n",
    "                        except Exception:\n",
    "                            # fallback to light rolling average if sector spline fails\n",
    "                            ideal_df.loc[sdata.index, 'X_smooth'] = sdata['X'].rolling(window=2, min_periods=1).mean()\n",
    "                            ideal_df.loc[sdata.index, 'Y_smooth'] = sdata['Y'].rolling(window=2, min_periods=1).mean()\n",
    "                    else:\n",
    "                        # too few points to spline  just copy the raw points\n",
    "                        ideal_df.loc[sdata.index, 'X_smooth'] = sdata['X']\n",
    "                        ideal_df.loc[sdata.index, 'Y_smooth'] = sdata['Y']\n",
    "        return ideal_df, sector_perf_df\n",
    "\n",
    "    def plot_ideal_line(ideal_line_df, sector_perf_df, full_track_df, vehicle_id_to_map):\n",
    "        fig, ax = plt.subplots(figsize=(12, 8))\n",
    "        ax.plot(full_track_df['X'], full_track_df['Y'], color='lightgray', linewidth=1, alpha=0.4, label='All Laps Trace')\n",
    "        \n",
    "        sector_colors = {1: 'red', 2: 'blue', 3: 'green'}\n",
    "\n",
    "        # We plot the pre-smoothed data\n",
    "        for sector_num in [1, 2, 3]:\n",
    "            sector_data = ideal_line_df[ideal_line_df['Sector'] == sector_num].copy()\n",
    "            \n",
    "            if not sector_data.empty:\n",
    "                source_lap = int(sector_data['SourceLap'].iloc[0]) \n",
    "                \n",
    "                # Plot using the pre-calculated SMOOTH_X and SMOOTH_Y columns\n",
    "                ax.plot(sector_data['X_smooth'], sector_data['Y_smooth'], \n",
    "                        color=sector_colors[sector_num], \n",
    "                        linewidth=3, \n",
    "                        label=f'Sector {sector_num} (Lap {source_lap})')\n",
    "        \n",
    "        ax.set_aspect('equal', adjustable='box')\n",
    "        plt.title(f'Theoretical Best Lap - Combined Best Sectors\\nVehicle {vehicle_id_to_map}')\n",
    "        plt.xlabel('Local X Coordinate (meters)')\n",
    "        plt.ylabel('Local Y Coordinate (meters)')\n",
    "        plt.legend()\n",
    "        plt.grid(True, alpha=0.3)\n",
    "        plt.show()\n",
    "\n",
    "\n",
    "    # Call the functions to generate the ideal line plot\n",
    "    result = calculate_ideal_line_by_sectors(full_track_df)\n",
    "    if result is not None:\n",
    "        ideal_path_df, sector_perf_df = result\n",
    "        plot_ideal_line(ideal_path_df, sector_perf_df, full_track_df, vehicle_id_to_map)\n",
    "    else:\n",
    "        print(\"Skipping ideal line plot due to calculation errors.\")\n",
    "\n",
    "    # --- Plotting Function (Uses full_track_df for complete view) ---\n",
    "    def plot_telemetry_metric(df, metric_column, title, draw_pits=False, cmap_name='viridis'):\n",
    "        df_plot = df.copy() \n",
    "        x = df_plot['X']\n",
    "        y = df_plot['Y']\n",
    "        metric_values = df_plot[metric_column]\n",
    "        if x.empty or y.empty or metric_values.empty:\n",
    "            print(f\"Plotting skipped for {metric_column}: Data is empty.\")\n",
    "            return\n",
    "        points = np.array([x, y]).T.reshape(-1, 1, 2)\n",
    "        segments = np.concatenate([points[:-1], points[1:]], axis=1)\n",
    "        fig, ax = plt.subplots(figsize=(10, 7))\n",
    "        if metric_column == 'Gear':\n",
    "            unique_gears = sorted(df_plot['Gear'].dropna().unique())\n",
    "            cmap = ListedColormap(['blue', 'green', 'yellow', 'orange', 'red', 'purple', 'black'])\n",
    "            norm = BoundaryNorm(unique_gears + [unique_gears[-1] + 1], cmap.N)\n",
    "        else:\n",
    "            v_min = metric_values.min()\n",
    "            v_max = metric_values.max()\n",
    "            if v_min == v_max:\n",
    "                norm = plt.Normalize(v_min, v_max + 0.01)\n",
    "            else:\n",
    "                norm = plt.Normalize(v_min, v_max)\n",
    "\n",
    "\n",
    "            cmap = cmap_name\n",
    "\n",
    "        lc = LineCollection(segments, cmap=cmap, norm=norm)\n",
    "        lc.set_array(metric_values)\n",
    "        lc.set_linewidth(3)\n",
    "        line = ax.add_collection(lc)\n",
    "        plt.colorbar(line, ax=ax, label=metric_column)\n",
    "\n",
    "        ax.set_xlim(telemetry_wide_df['X'].min(), telemetry_wide_df['X'].max())\n",
    "        ax.set_ylim(telemetry_wide_df['Y'].min(), telemetry_wide_df['Y'].max())\n",
    "        ax.set_aspect('equal', adjustable='box')\n",
    "        \n",
    "        if draw_pits:\n",
    "            pit_entry_x = (pit_entry_lon - min_lon) * scale_factor\n",
    "            pit_entry_y = (pit_entry_lat - min_lat) * scale_factor\n",
    "            pit_exit_x = (pit_exit_long - min_lon) * scale_factor\n",
    "            pit_exit_y = (pit_exit_lat - min_lat) * scale_factor\n",
    "            \n",
    "            ax.scatter(pit_entry_x, pit_entry_y, color='orange', marker='s', s=100, label='Pit Entry', zorder=5)\n",
    "            ax.scatter(pit_exit_x, pit_exit_y, color='red', marker='s', s=100, label='Pit Exit', zorder=5)\n",
    "            ax.legend()\n",
    "\n",
    "        plt.title(f'{title} around Barber Motorsports Park (All Laps for {vehicle_id_to_map})')\n",
    "        plt.xlabel('Local X Coordinate (meters)')\n",
    "        plt.ylabel('Local Y Coordinate (meters)')\n",
    "        plt.grid(False)\n",
    "        plt.show()\n",
    "\n",
    "    # --- Generate visualizations with continuous colormaps using original logic ---\n",
    "    #plot_telemetry_metric(full_track_df, 'Gear', 'Gear Selection Map', draw_pits=True)\n",
    "    #plot_telemetry_metric(full_track_df, 'Throttle', 'Throttle Position Map (%)', draw_pits=True, cmap_name='plasma')\n",
    "    #plot_telemetry_metric(full_track_df, 'BrakeIntensity', 'Braking Intensity (%)', draw_pits=True, cmap_name='Reds')\n",
    "    #plot_telemetry_metric(full_track_df, 'Speed_kph', 'Speed Map (km/h)', draw_pits=True, cmap_name='jet')\n",
    "    #plot_telemetry_metric(full_track_df, 'G_Long', 'Longitudinal Acceleration (G)', draw_pits=True, cmap_name='coolwarm')"
   ]
