   "cell_type": "markdown",
   "id": "467ade97",
   "metadata": {},
   "source": [
    "# No spline smooth #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "39dab244",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Using assembled variable 'ideal_clean' for non-spline smoothing.\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Alternative: smooth the assembled ideal WITHOUT using splprep/splev (no splines)\n",
    "# This resamples each sector by cumulative distance, applies per-sector smoothing (Savitzky or rolling),\n",
    "# and blends seams with a configurable blend width. Use this instead of the spline-based smoothing\n",
    "# if you want a robust, non-spline seam-free final path.\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "from scipy.signal import savgol_filter\n",
    "from scipy.interpolate import interp1d\n",
    "\n",
    "\n",
    "def smooth_ideal_no_spline(assembled_df, upsample_factor=3, method='savgol', window=11, polyorder=2, blend=7):\n",
    "    \"\"\"\n",
    "    Smooth assembled ideal without using splprep/splev.\n",
    "\n",
    "    Steps:\n",
    "    - For each sector, sort by Normalized_Index and compute cumulative distance along that sector.\n",
    "    - Resample each sector at higher resolution (upsample_factor) using linear interpolation over distance.\n",
    "    - Apply per-sector smoothing (Savitzky-Golay or centered rolling mean).\n",
    "    - Blend seams between sectors by linearly interpolating across `blend` points.\n",
    "\n",
    "    Parameters\n",
    "    ----------\n",
    "    assembled_df : pd.DataFrame\n",
    "        DataFrame that contains at least columns ['X','Y','Sector','Normalized_Index'] (X/Y numeric)\n",
    "        It may already have X_smooth/Y_smooth; we resample the X/Y coordinates directly.\n",
    "    upsample_factor : int\n",
    "        How many times to upsample each sector relative to its original point count.\n",
    "    method : {'savgol','rolling'}\n",
    "        Smoothing method applied per-sector to the resampled coordinates.\n",
    "    window : int\n",
    "        Window length for smoothing (will be adjusted to valid odd value for savgol).\n",
    "    polyorder : int\n",
    "        Savitzky-Golay polynomial order.\n",
    "    blend : int\n",
    "        Number of points to blend at each sector seam. If 0, seams are not specially blended.\n",
    "\n",
    "    Returns\n",
    "    -------\n",
    "    out_df : pd.DataFrame\n",
    "        DataFrame with columns ['X_final','Y_final','Sector','resample_pos'] representing\n",
    "        the smoothed, stitched path ready for plotting. Index is sequential along the stitched path.\n",
    "    \"\"\"\n",
    "    df = assembled_df.copy()\n",
    "    required = {'X','Y','Sector','Normalized_Index'}\n",
    "    if not required.issubset(set(df.columns)):\n",
    "        raise ValueError(f\"assembled_df must contain columns: {required}\")\n",
    "\n",
    "    sectors = sorted(df['Sector'].dropna().unique())\n",
    "    stitched = []\n",
    "\n",
    "    for s in sectors:\n",
    "        sdata = df[df['Sector'] == s].sort_values('Normalized_Index')\n",
    "        if sdata.shape[0] == 0:\n",
    "            continue\n",
    "\n",
    "        x = sdata['X'].astype(float).to_numpy()\n",
    "        y = sdata['Y'].astype(float).to_numpy()\n",
    "\n",
    "        # compute cumulative distance\n",
    "        if len(x) == 1:\n",
    "            cum = np.array([0.0])\n",
    "        else:\n",
    "            dx = np.diff(x)\n",
    "            dy = np.diff(y)\n",
    "            segd = np.sqrt(dx*dx + dy*dy)\n",
    "            cum = np.concatenate([[0.0], np.cumsum(segd)])\n",
    "\n",
    "        # define resample count\n",
    "        n_orig = max(2, len(x))\n",
    "        n_resample = max(3, int(n_orig * upsample_factor))\n",
    "        new_pos = np.linspace(0.0, cum[-1], n_resample)\n",
    "\n",
    "        # linear interpolation over distance\n",
    "        if len(cum) == 1:\n",
    "            x_new = np.full_like(new_pos, x[0], dtype=float)\n",
    "            y_new = np.full_like(new_pos, y[0], dtype=float)\n",
    "        else:\n",
    "            fx = interp1d(cum, x, kind='linear', bounds_error=False, fill_value=(x[0], x[-1]))\n",
    "            fy = interp1d(cum, y, kind='linear', bounds_error=False, fill_value=(y[0], y[-1]))\n",
    "            x_new = fx(new_pos)\n",
    "            y_new = fy(new_pos)\n",
    "\n",
    "        # smoothing\n",
    "        if method == 'savgol':\n",
    "            w = int(window)\n",
    "            if w % 2 == 0:\n",
    "                w += 1\n",
    "            if w >= n_resample:\n",
    "                w = n_resample if n_resample % 2 == 1 else n_resample - 1\n",
    "                if w < 3:\n",
    "                    w = 3\n",
    "            if w <= polyorder:\n",
    "                w = polyorder + 2 + (polyorder + 2) % 2\n",
    "            try:\n",
    "                x_s = savgol_filter(x_new, window_length=w, polyorder=polyorder, mode='interp')\n",
    "                y_s = savgol_filter(y_new, window_length=w, polyorder=polyorder, mode='interp')\n",
    "            except Exception:\n",
    "                # fallback to simple rolling mean\n",
    "                x_s = pd.Series(x_new).rolling(window=3, center=True, min_periods=1).mean().to_numpy()\n",
    "                y_s = pd.Series(y_new).rolling(window=3, center=True, min_periods=1).mean().to_numpy()\n",
    "        elif method == 'rolling':\n",
    "            w = int(window)\n",
    "            if w < 1:\n",
    "                w = 1\n",
    "            x_s = pd.Series(x_new).rolling(window=w, center=True, min_periods=1).mean().to_numpy()\n",
    "            y_s = pd.Series(y_new).rolling(window=w, center=True, min_periods=1).mean().to_numpy()\n",
    "        else:\n",
    "            raise ValueError(\"method must be 'savgol' or 'rolling'\")\n",
    "\n",
    "        # record resampled position along sector (for later optional mapping)\n",
    "        sector_df = pd.DataFrame({'X_res': x_new, 'Y_res': y_new, 'X_smooth_sec': x_s, 'Y_smooth_sec': y_s, 'Sector': s, 'pos': new_pos})\n",
    "        stitched.append(sector_df)\n",
    "\n",
    "    if not stitched:\n",
    "        return pd.DataFrame(columns=['X_final','Y_final','Sector','resample_pos'])\n",
    "\n",
    "    # concatenate sectors\n",
    "    full = pd.concat(stitched, ignore_index=True)\n",
    "\n",
    "    # Now blend at seams\n",
    "    if blend > 0 and len(sectors) > 1:\n",
    "        idx = 0\n",
    "        # compute sector boundaries in full\n",
    "        boundaries = []  # list of (start_idx, end_idx) for each sector in full\n",
    "        for s in sectors:\n",
    "            seg_len = (full['Sector'] == s).sum()\n",
    "            start = idx\n",
    "            end = idx + seg_len - 1\n",
    "            boundaries.append((start, end))\n",
    "            idx += seg_len\n",
    "\n",
    "        X_final = full['X_smooth_sec'].to_numpy().astype(float)\n",
    "        Y_final = full['Y_smooth_sec'].to_numpy().astype(float)\n",
    "\n",
    "        for i in range(len(boundaries)-1):\n",
    "            a_start, a_end = boundaries[i]\n",
    "            b_start, b_end = boundaries[i+1]\n",
    "            # pick last `blend` points of A and first `blend` points of B\n",
    "            a_inds = np.arange(max(a_start, a_end - blend + 1), a_end + 1)\n",
    "            b_inds = np.arange(b_start, min(b_end, b_start + blend - 1) + 1)\n",
    "\n",
    "            # if either side has fewer than 1, skip\n",
    "            if len(a_inds) == 0 or len(b_inds) == 0:\n",
    "                continue\n",
    "\n",
    "            # Make both arrays equal length by padding/truncating to min_len\n",
    "            min_len = min(len(a_inds), len(b_inds))\n",
    "            a_inds = a_inds[-min_len:]\n",
    "            b_inds = b_inds[:min_len]\n",
    "\n",
    "            for j, (ai, bi) in enumerate(zip(a_inds, b_inds)):\n",
    "                alpha = (j + 1) / (min_len + 1)  # 0..1 blending across seam\n",
    "                X_final[ai] = (1 - alpha) * X_final[ai] + alpha * X_final[bi]\n",
    "                Y_final[ai] = (1 - alpha) * Y_final[ai] + alpha * Y_final[bi]\n",
    "                X_final[bi] = X_final[ai]  # optionally make symmetric\n",
    "                Y_final[bi] = Y_final[ai]\n",
    "\n",
    "        out = pd.DataFrame({'X_final': X_final, 'Y_final': Y_final, 'Sector': full['Sector'].to_numpy(), 'resample_pos': np.arange(len(X_final))})\n",
    "    else:\n",
    "        out = pd.DataFrame({'X_final': full['X_smooth_sec'].to_numpy(), 'Y_final': full['Y_smooth_sec'].to_numpy(), 'Sector': full['Sector'].to_numpy(), 'resample_pos': np.arange(len(full))})\n",
    "\n",
    "    return out\n",
    "\n",
    "\n",
    "# Example usage: auto-detect assembled ideal variable and run non-spline smoothing + plot\n",
    "try:\n",
    "    assembled_candidates = ['ideal_clean', 'ideal_sm', 'ideal_top', 'ideal_path_df_v2', 'ideal_path_df', 'ideal_raw', 'ideal_path_df_v2']\n",
    "    assembled_var = None\n",
    "    for name in assembled_candidates:\n",
    "        if name in globals():\n",
    "            assembled_var = name\n",
    "            break\n",
    "    if assembled_var is None:\n",
    "        print('No assembled ideal variable found. Run v2 recompute cell first or set assembled_df variable and re-run this cell.')\n",
    "    else:\n",
    "        print(f\"Using assembled variable '{assembled_var}' for non-spline smoothing.\")\n",
    "        assembled_df = globals()[assembled_var].copy()\n",
    "        # Run smoothing without splines\n",
    "        result_no_spline = smooth_ideal_no_spline(assembled_df, upsample_factor=4, method='savgol', window=11, polyorder=2, blend=9)\n",
    "\n",
    "        # quick overlay plot\n",
    "        import matplotlib.pyplot as plt\n",
    "        plt.figure(figsize=(10,6))\n",
    "        plt.plot(full_track_df['X'], full_track_df['Y'], color='lightgray', alpha=0.4, label='All Laps')\n",
    "        # plot pre-final (if X_smooth exists) otherwise raw\n",
    "        srcx = 'X_smooth' if 'X_smooth' in assembled_df.columns else 'X'\n",
    "        srcy = 'Y_smooth' if 'Y_smooth' in assembled_df.columns else 'Y'\n",
    "        plt.plot(assembled_df[srcx], assembled_df[srcy], color='gray', linewidth=2, label='assembled (pre-final)')\n",
    "        plt.plot(result_no_spline['X_final'], result_no_spline['Y_final'], color='red', linewidth=2, label='no-spline final')\n",
    "        plt.axis('equal')\n",
    "        plt.legend()\n",
    "        plt.title('Non-spline final smoothing result')\n",
    "        plt.show()\n",
    "except Exception as e:\n",
    "    print('Non-spline smoothing cell inserted but example run failed:', e)\n"
   ]
  },
