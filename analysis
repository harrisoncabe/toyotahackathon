   ],
   "source": [
    "\n",
    "# This cell produces `ideal_final_df` using the previously defined wrap_stitch_and_blend helper,\n",
    "# then plots the result with sector colors and stores it for downstream use.\n",
    "\n",
    "try:\n",
    "    # auto-detect assembled ideal variable (common names)\n",
    "    candidates = ['ideal_final_df','ideal_clean','ideal_sm','ideal_top','ideal_path_df_v2','ideal_path_df','ideal_raw','ideal_sm']\n",
    "    assembled_name = None\n",
    "    for n in candidates:\n",
    "        if n in globals():\n",
    "            assembled_name = n\n",
    "            break\n",
    "    if assembled_name is None:\n",
    "        # fall back to any ideal-like variable present\n",
    "        for v in ['ideal_path_df_v2','ideal_path_df','ideal_raw','ideal_sm','ideal_clean']:\n",
    "            if v in globals():\n",
    "                assembled_name = v\n",
    "                break\n",
    "\n",
    "    if assembled_name is None:\n",
    "        raise NameError('No assembled ideal variable found in kernel. Run the v2 recompute cell first.')\n",
    "\n",
    "    assembled = globals()[assembled_name].copy()\n",
    "    print(f\"Using assembled variable: {assembled_name} (rows={len(assembled)})\")\n",
    "\n",
    "    # run wrap-aware stitch and blend\n",
    "    # wrap_stitch_and_blend should already be defined in the notebook\n",
    "    stitched, info = wrap_stitch_and_blend(assembled, blend=8)                                                 # 8 or 12 is best setting, under 7 makes it not blend\n",
    "    print('Stitch info:', info)\n",
    "\n",
    "    # produce canonical final df\n",
    "    ideal_final_df = stitched.copy()\n",
    "    # ensure Sector column exists and is integer\n",
    "    if 'Sector' in ideal_final_df.columns:\n",
    "        # coerce to int where possible\n",
    "        try:\n",
    "            ideal_final_df['Sector'] = ideal_final_df['Sector'].astype(int)\n",
    "        except Exception:\n",
    "            ideal_final_df['Sector'] = ideal_final_df['Sector'].round(0).astype(int)\n",
    "    else:\n",
    "        # if Sector missing, attempt to reconstruct from Normalized_Index using sector boundaries\n",
    "        if 'Normalized_Index' in ideal_final_df.columns and 'full_track_df' in globals():\n",
    "            TARGET_S1_DIST_IN = 40512\n",
    "            TARGET_S2_DIST_IN = 102732\n",
    "            s1_boundary_points = full_track_df[(full_track_df['LapDist_in'].notna()) & (full_track_df['LapDist_in'] >= TARGET_S1_DIST_IN - 500) & (full_track_df['LapDist_in'] <= TARGET_S1_DIST_IN + 500)]\n",
    "            s2_boundary_points = full_track_df[(full_track_df['LapDist_in'].notna()) & (full_track_df['LapDist_in'] >= TARGET_S2_DIST_IN - 500) & (full_track_df['LapDist_in'] <= TARGET_S2_DIST_IN + 500)]\n",
    "            s1_end_idx = int(round(s1_boundary_points['Normalized_Index'].median())) if not s1_boundary_points.empty else int(full_track_df['Normalized_Index'].max() / 3)\n",
    "            s2_end_idx = int(round(s2_boundary_points['Normalized_Index'].median())) if not s2_boundary_points.empty else int((full_track_df['Normalized_Index'].max() / 3) * 2)\n",
    "            def assign_sector(idx):\n",
    "                if idx <= s1_end_idx: return 1\n",
    "                elif idx <= s2_end_idx: return 2\n",
    "                else: return 3\n",
    "            ideal_final_df['Sector'] = ideal_final_df['Normalized_Index'].apply(assign_sector).astype(int)\n",
    "        else:\n",
    "            ideal_final_df['Sector'] = 1\n",
    "\n",
    "    # expose ideal_final_df in globals for downstream use\n",
    "    globals()['ideal_final_df'] = ideal_final_df\n",
    "\n",
    "    # Plot sector-colored final result using contiguous runs to avoid connecting non-consecutive points\n",
    "    fig, ax = plt.subplots(figsize=(12,8))\n",
    "    ax.plot(full_track_df['X'], full_track_df['Y'], color='lightgray', linewidth=1, alpha=0.4, label='All Laps Trace')\n",
    "\n",
    "    sector_colors = {1: 'red', 2: 'blue', 3: 'green'}\n",
    "\n",
    "    # Iterate through ideal_final_df in sequence and plot contiguous runs where Sector is constant\n",
    "    coords = ideal_final_df[['X_final','Y_final','Sector']].reset_index(drop=True)\n",
    "    n = len(coords)\n",
    "    if n == 0:\n",
    "        raise ValueError('ideal_final_df is empty')\n",
    "\n",
    "    runs = []\n",
    "    start_idx = 0\n",
    "    current_sector = coords.loc[0,'Sector']\n",
    "    for i in range(1, n):\n",
    "        if coords.loc[i,'Sector'] != current_sector:\n",
    "            run = coords.loc[start_idx:i-1, ['X_final','Y_final']]\n",
    "            runs.append((int(current_sector), run))\n",
    "            start_idx = i\n",
    "            current_sector = coords.loc[i,'Sector']\n",
    "    # append last run\n",
    "    run = coords.loc[start_idx:n-1, ['X_final','Y_final']]\n",
    "    runs.append((int(current_sector), run))\n",
    "\n",
    "    # plot each run separately\n",
    "    for s, run_df in runs:\n",
    "        if run_df.empty:\n",
    "            continue\n",
    "        xvals = run_df['X_final'].to_numpy()\n",
    "        yvals = run_df['Y_final'].to_numpy()\n",
    "        ax.plot(xvals, yvals, color=sector_colors.get(s, 'black'), linewidth=3, label=f'Sector {s}')\n",
    "        # mark start/end for the run\n",
    "        #ax.scatter(xvals[0], yvals[0], marker='x', color=sector_colors.get(s,'black'), s=60)\n",
    "        #ax.scatter(xvals[-1], yvals[-1], marker='o', color=sector_colors.get(s,'black'), s=60)\n",
    "\n",
    "    ax.set_aspect('equal', adjustable='box')\n",
    "    ax.set_title('Final Theoretical Best Lap (wrap-aware stitched, sector colors)')\n",
    "    # avoid duplicate legend entries\n",
    "    handles, labels = ax.get_legend_handles_labels()\n",
    "    by_label = dict(zip(labels, handles))\n",
    "    ax.legend(by_label.values(), by_label.keys())\n",
    "    plt.show()\n",
    "\n",
    "    print('Saved final stitched ideal as `ideal_final_df` (use X_final/Y_final for plotting)')\n",
    "\n",
    "except Exception as e:\n",
    "    print('Final wrap-aware plotting cell failed:', e)\n"
   ]
