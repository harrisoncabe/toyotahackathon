    "# Run this cell after you run the cell that computes `ideal_path_df` (result of calculate_ideal_line_by_sectors)\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "def run_ideal_line_diagnostics(ideal_df, full_df=None, show_plot=True):\n",
    "    \"\"\"Prints sector counts, gap distances between sector boundaries and optionally overlays raw sector points on the full track.\n",
    "\n",
    "    Usage: run_ideal_line_diagnostics(ideal_path_df, full_track_df)\n",
    "    \"\"\"\n",
    "    if ideal_df is None or ideal_df.empty:\n",
    "        print(\"No ideal_df provided or it's empty.\")\n",
    "        return\n",
    "\n",
    "    counts = {n: len(ideal_df[ideal_df['Sector'] == n]) for n in [1, 2, 3]}\n",
    "    print(\"Sector point counts:\", counts)\n",
    "\n",
    "    # Compute Euclidean gaps between end of sector N and start of sector N+1\n",
    "    gaps = {}\n",
    "    for a, b in [(1, 2), (2, 3)]:\n",
    "        s_a = ideal_df[ideal_df['Sector'] == a]\n",
    "        s_b = ideal_df[ideal_df['Sector'] == b]\n",
    "        if not s_a.empty and not s_b.empty:\n",
    "            pa = s_a[['X', 'Y']].iloc[-1].values.astype(float)\n",
    "            pb = s_b[['X', 'Y']].iloc[0].values.astype(float)\n",
    "            gaps[f\"{a}-{b}\"] = float(np.linalg.norm(pa - pb))\n",
    "        else:\n",
    "            gaps[f\"{a}-{b}\"] = None\n",
    "    print(\"Sector boundary gaps (meters):\", gaps)\n",
    "\n",
    "    # Show Normalized_Index ranges per sector to detect ordering discontinuities\n",
    "    ranges = {}\n",
    "    for n in [1, 2, 3]:\n",
    "        s = ideal_df[ideal_df['Sector'] == n]\n",
    "        if not s.empty:\n",
    "            ranges[n] = (float(s['Normalized_Index'].min()), float(s['Normalized_Index'].max()))\n",
    "        else:\n",
    "            ranges[n] = None\n",
    "    print(\"Normalized_Index ranges per sector:\", ranges)\n",
    "\n",
    "    if show_plot and full_df is not None:\n",
    "        try:\n",
    "            fig, ax = plt.subplots(figsize=(9, 6))\n",
    "            # background - full dataset trace\n",
    "            ax.plot(full_df['X'], full_df['Y'], color='lightgray', linewidth=1, alpha=0.5, label='All Laps Trace (background)')\n",
    "            sector_colors = {1: 'red', 2: 'blue', 3: 'green'}\n",
    "            for n in [1, 2, 3]:\n",
    "                s = ideal_df[ideal_df['Sector'] == n]\n",
    "                if not s.empty:\n",
    "                    ax.scatter(s['X'], s['Y'], s=12, c=sector_colors[n], alpha=0.8, label=f'Sector {n} raw')\n",
    "                    # mark endpoints for quick visual check\n",
    "                    ax.scatter(s['X'].iloc[0], s['Y'].iloc[0], marker='x', c=sector_colors[n], s=60)\n",
    "                    ax.scatter(s['X'].iloc[-1], s['Y'].iloc[-1], marker='o', c=sector_colors[n], s=60)\n",
    "            ax.set_aspect('equal', adjustable='box')\n",
    "            ax.set_title('Diagnostics: Raw sector points over full track')\n",
    "            ax.legend()\n",
    "            plt.show()\n",
    "        except Exception as e_plot:\n",
    "            print('Diagnostic plot failed:', e_plot)\n",
    "    else:\n",
    "        print(\"Full track df not provided; skipping overlay plot.\")\n",
    "\n",
    "\n",
    "# Auto-run diagnostics if the expected variables exist in the kernel\n",
    "try:\n",
    "    run_ideal_line_diagnostics(ideal_path_df, full_track_df)\n",
    "except NameError:\n",
    "    print(\"Variables `ideal_path_df` or `full_track_df` not found in the current kernel. Run the main cell that calculates the ideal line first and then run this diagnostics cell.\")\n"
   ]
